{
  "name": "@dogeai/agent",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "prebuild": "pnpm run check:types",
    "build": "esbuild --bundle --platform=node --outdir=dist ./src/server.ts ./src/chat-server.ts '--external:@libsql/client'",
    "predeploy": "pnpm run build",
    "check:types": "tsc --noEmit",
    "deploy": "fly deploy",
    "prompt:update": "tsx --watch ./src/temp/update-prompt-db.ts",
    "prompt:migrate": "tsx --watch --env-file=.env ./src/temp/migrate-prompts.ts",
    "terminal:userlimit": "tsx --watch --env-file=.env ./src/temp/update-user-chat-limit.ts",
    "twitter:thread": "tsx src/temp/generate-thread.ts",
    "twitter:replies": "tsx --watch --env-file=.env ./src/temp/reply-testing.ts",
    "twitter:reply": "tsx --watch --env-file=.env ./src/temp/reply-twitter.ts",
    "twitter:tag": "tsx --watch --env-file=.env ./src/temp/tag-twitter.ts",
    "twitter:engage": "tsx --watch --env-file=.env ./src/temp/engage-twitter.ts",
    "dev:chat": "tsx --watch --env-file=.env ./src/chat-server.ts | pino-pretty",
    "dev:server": "tsx --watch --env-file=.env ./src/server.ts | pino-pretty",
    "dev:inngest": "inngest dev -u http://localhost:3000/api/inngest",
    "dev:agent": "pnpm run dev:server & pnpm run dev:inngest",
    "db:generate:chat": "drizzle-kit generate --config=./drizzle-chat.config.ts",
    "db:generate:controlplane": "drizzle-kit generate --config=./drizzle-controlplane.config.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/deepinfra": "^0.2.17",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/perplexity": "^1.1.9",
    "@ai-sdk/xai": "^1.2.18",
    "@fastify/cors": "^11.0.1",
    "@langchain/textsplitters": "^0.1.0",
    "@libsql/client": "^0.14.0",
    "@openrouter/ai-sdk-provider": "^0.7.2",
    "@sinclair/typebox": "^0.34.38",
    "@trpc/server": "^11.2.0",
    "@tursodatabase/api": "^1.9.0",
    "ai": "^4.3.19",
    "bentocache": "1.0.0-beta.9",
    "database": "workspace:^",
    "date-fns": "^4.1.0",
    "dedent": "^1.6.0",
    "discord.js": "^14.21.0",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.3",
    "exa-js": "^1.8.23",
    "fastify": "5.4.0",
    "fastify-plugin": "^5.0.1",
    "handlebars": "^4.7.8",
    "inngest": "^3.40.1",
    "ioredis": "^5.6.1",
    "jose": "^6.0.11",
    "lodash-es": "^4.17.21",
    "p-map": "^7.0.3",
    "pino": "^9.7.0",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^7.1.1",
    "readline": "^1.3.0",
    "slugify": "^1.6.6",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tsx": "^4.20.3",
    "twitter-api-v2": "^1.19.0",
    "typescript": "^5.8.3",
    "yargs": "17.7.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/yargs": "^17.0.33",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.8",
    "inngest-cli": "1.8.2",
    "pino-pretty": "^13.0.0"
  }
}
