{
  "name": "@dogeai/agent",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "esbuild --bundle --platform=node --outdir=dist ./src/server.ts '--external:@libsql/client'",
    "predeploy": "pnpm run build",
    "deploy": "fly deploy",
    "dev:tweet": "tsx src/temp/another-take.ts",
    "twitter": "tsx --watch --env-file=.env ./src/temp/reply-testing.ts",
    "test-kb": "tsx --watch ./src/temp/test-kb.ts",
    "dev:server": "tsx --watch --env-file=.env ./src/server.ts",
    "dev:inngest": "inngest dev -u http://localhost:3000/api/inngest",
    "dev:agent": "pnpm run dev:server & pnpm run dev:inngest"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.19",
    "@ai-sdk/xai": "^1.0.17",
    "@langchain/textsplitters": "^0.1.0",
    "ai": "^4.0.22",
    "bentocache": "1.0.0-beta.9",
    "database": "workspace:^",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "handlebars": "^4.7.8",
    "inngest": "^3.30.0",
    "lodash-es": "^4.17.21",
    "readline": "^1.3.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tsx": "^4.19.2",
    "twitter-api-v2": "^1.19.0",
    "typescript": "^5.7.2",
    "yargs": "17.7.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/yargs": "^17.0.33",
    "esbuild": "^0.24.2",
    "inngest-cli": "^1.3.3"
  }
}
