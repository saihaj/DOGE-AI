{
  "name": "@dogeai/agent",
  "version": "0.0.1",
  "main": "src/index.ts",
  "scripts": {
    "start": "tsx src/index.ts",
    "prebuild": "pnpm run check:types",
    "build": "esbuild --bundle --platform=node --outdir=dist ./src/server.ts '--external:@libsql/client'",
    "predeploy": "pnpm run build",
    "check:types": "tsc --noEmit",
    "deploy": "fly deploy",
    "prompt:update": "tsx --watch ./src/temp/update-prompt-db.ts",
    "twitter:thread": "tsx src/temp/generate-thread.ts",
    "twitter:replies": "tsx --watch --env-file=.env ./src/temp/reply-testing.ts",
    "twitter:replynext": "tsx --watch --env-file=.env ./src/temp/reply-twitter.ts",
    "twitter:engage": "tsx --watch --env-file=.env ./src/temp/engage-twitter.ts",
    "dev:server": "tsx --watch --env-file=.env ./src/server.ts",
    "dev:inngest": "inngest dev -u http://localhost:3000/api/inngest",
    "dev:agent": "pnpm run dev:server & pnpm run dev:inngest"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^0.1.6",
    "@ai-sdk/google": "^1.1.6",
    "@ai-sdk/openai": "^1.1.9",
    "@ai-sdk/perplexity": "^0.0.4",
    "@ai-sdk/xai": "^1.1.6",
    "@fastify/cors": "^10.0.2",
    "@langchain/textsplitters": "^0.1.0",
    "@sinclair/typebox": "^0.34.16",
    "ai": "^4.1.10",
    "bentocache": "1.0.0-beta.9",
    "database": "workspace:^",
    "date-fns": "^4.1.0",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "fastify": "^5.2.0",
    "handlebars": "^4.7.8",
    "inngest": "^3.30.0",
    "lodash-es": "^4.17.21",
    "p-map": "^7.0.3",
    "readline": "^1.3.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tsx": "^4.19.2",
    "twitter-api-v2": "^1.19.0",
    "typescript": "^5.7.3",
    "yargs": "17.7.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/yargs": "^17.0.33",
    "esbuild": "^0.24.2",
    "inngest-cli": "^1.4.3"
  }
}
